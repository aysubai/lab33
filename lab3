import math
n=int(input("Massivin olcusunu daxil edin: "))
S=0
A=[]
for i in range(n):
    a=[]
    for j in range(n):
        x=int(input("Massivin [{i+1}][{j+1}] elementini daxil edin:"))
        a.append(x)
        A.append(a)
for i in range(n):
            S+=math.sqrt(A[i][i])
print("Bas diaqonal elementlerinin kvadrat kokleri cemii:",S)
